{
    "name": "codeigniter4/appstarter",
    "type": "project",
    "description": "The CodeIgniter 4 Application Starter",
    "homepage": "https://codeigniter.com",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "docs": "https://codeigniter4.github.io/userguide/"
    }
}


